{
	"name": "mmrtonmoybd/bd-payments",
	"description": "Bangladeshi Payment extension for Aimeos e-commerce solutions",
	"keywords": ["aimeos", "payment", "shop", "e-commerce", "sslcommerz", "aamarpay", "bkash", "nagad"],
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.1",
		"aimeos/aimeos-core": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~10.0||~11.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}